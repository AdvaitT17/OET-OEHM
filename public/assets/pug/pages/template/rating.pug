- var theme_customizer      = true;
- var rating                = true;
- var emoji_rating          = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
- var height_equal          = true;
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-6
                  h4
                    | Rating
                .col-6
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Bonus Ui
                    li.breadcrumb-item.active Rating
          // Container-fluid starts
          .container-fluid
            .row
              .col-xxl-4.col-md-6
                .card
                  .card-header.rating-header
                    h4 1 to 10 Rating Bar
                    p.f-m-light.mt-1
                      | Rating is displayed using 
                      code #u-rating-1to10
                      |  id using javascript.
                  .card-body
                    .rating-container
                      select#u-rating-1to10(name='rating', autocomplete='off')
                        option(value='1') 1
                        option(value='2') 2
                        option(value='3') 3
                        option(value='4') 4
                        option(value='5') 5
                        option(value='6') 6
                        option(value='7', selected='selected') 7
                        option(value='8') 8
                        option(value='9') 9
                        option(value='10') 10
              .col-xxl-4.col-md-6
                .card
                  .card-header
                    h4 Movie Rating Bar 
                    p.f-m-light.mt-1
                      | Movie rating is displayed using 
                      code #u-rating-movie
                      |  id using javascript.
                  .card-body
                    .rating-container
                      select#u-rating-movie(name='rating', autocomplete='off')
                        option(value='Bad') Bad
                        option(value='Mediocre') Mediocre
                        option(value='Good', selected='selected') Good
                        option(value='Awesome') Awesome
              .col-xxl-4.col-md-6
                .card
                  .card-header
                    h4 Square Rating Bar
                    p.f-m-light.mt-1
                      | Square rating is displayed using 
                      code #u-rating-square 
                      |  id using javascript.
                  .card-body
                    .rating-container
                      select#u-rating-square(name='rating', autocomplete='off')
                        option(value='1') 1
                        option(value='2') 2
                        option(value='3') 3
                        option(value='4') 4
                        option(value='5') 5
              .col-xxl-4.col-md-6
                .card
                  .card-header
                    h4 Pill Rating Bar 
                    p.f-m-light.mt-1
                      | Pill rating is displayed using 
                      code #u-rating-pill 
                      |  id using javascript.
                  .card-body
                    .rating-container
                      select#u-rating-pill(name='rating', autocomplete='off')
                        option(value='A') A 
                        option(value='B') B
                        option(value='C') C
                        option(value='D') D
                        option(value='E') E
                        option(value='F') F 
              .col-xxl-4.col-md-6
                .card
                  .card-header
                    h4 Reversed Rating Bar 
                    p.f-m-light.mt-1
                      | Reversed rating is displayed using 
                      code #u-rating-reversed  
                      |  id using javascript.
                  .card-body
                    .rating-container
                      select#u-rating-reversed(name='rating', autocomplete='off')
                        option(value='Strongly Agree') Strongly Agree
                        option(value='Agree') Agree
                        option(value='Neither Agree or Disagree', selected='selected') Neither Agree or Disagree
                        option(value='Disagree') Disagree
                        option(value='Strongly Disagree') Strongly Disagree
              .col-xxl-4.col-md-6
                .card
                  .card-header
                    h4 Star Rating Bar 
                    p.f-m-light.mt-1
                      | Star rating is displayed using 
                      code #u-rating-fontawesome 
                      |  id using javascript.
                  .card-body
                    .rating-container
                      select#u-rating-fontawesome(name='rating', autocomplete='off')
                        option(value='1') 1
                        option(value='2') 2
                        option(value='3') 3
                        option(value='4') 4
                        option(value='5') 5
              .col-xxl-4.col-md-6
                .card.height-equal
                  .card-header
                    h4 Horizontal Rating Bar 
                    p.f-m-light.mt-1
                      | Horizontal rating is displayed using 
                      code #u-rating-horizontal
                      |  id using javascript.
                  .card-body
                    .rating-container
                      select#u-rating-horizontal(name='rating', autocomplete='off')
                        option(value='10') 10
                        option(value='9') 9
                        option(value='8') 8
                        option(value='7') 7
                        option(value='6') 6
                        option(value='5') 5
                        option(value='4') 4
                        option(value='3') 3
                        option(value='2') 2
                        option(value='1', selected='selected') 1
              .col-xxl-4.col-md-6
                .card.height-equal
                  .card-header
                    h4 Current Rating Bar
                    p.f-m-light.mt-1
                      | Star rating is displayed using 
                      code #u-rating-fontawesome-o 
                      |  id using javascript.
                  .card-body
                    .rating-container
                      .star-ratings
                        .stars.stars-example-fontawesome-o
                          select#u-rating-fontawesome-o(name='rating', data-current-rating='5.6', autocomplete='off')
                            option(value='1') 1
                            option(value='2') 2
                            option(value='3') 3
                            option(value='4') 4
                            option(value='5') 5
                            option(value='6') 6
                            option(value='7') 7
                            option(value='8') 8
                            option(value='9') 9
                            option(value='10') 10
                          span.title.current-rating
                            | Current rating: 
                            span.value
                          span.title.your-rating.hidden
                            | Your rating: 
                            span.value
                            a.clear-rating(href='#')
                              i.fa.fa-times-circle
              .col-xxl-4.col-md-12
                .card.height-equal
                  .card-header
                    h4 Animated Rating Bar
                    p.f-m-light.mt-1
                      | Star rating is displayed using 
                      code #u-rating-fontawesome-o 
                      |  id using javascript.
                  .card-body
                    .rating-container
                      ul.feedback
                        li.angry.me-0
                          div
                            svg.eye.left
                              use(xlink:href='../assets/svg/icon-sprite.svg#eye')
                            svg.eye.right
                              use(xlink:href='../assets/svg/icon-sprite.svg#eye')
                            svg.mouth
                              use(xlink:href='../assets/svg/icon-sprite.svg#mouth')
                        li.sad.me-0
                          div
                            svg.eye.left
                              use(xlink:href='../assets/svg/icon-sprite.svg#eye')
                            svg.eye.right
                              use(xlink:href='../assets/svg/icon-sprite.svg#eye')
                            svg.mouth
                              use(xlink:href='../assets/svg/icon-sprite.svg#mouth')
                        li.ok.me-0
                          div
                        li.good.active.me-0
                          div
                            svg.eye.left
                              use(xlink:href='../assets/svg/icon-sprite.svg#eye')
                            svg.eye.right
                              use(xlink:href='../assets/svg/icon-sprite.svg#eye')
                            svg.mouth
                              use(xlink:href='../assets/svg/icon-sprite.svg#mouth')
                        li.happy.me-0
                          div
                            svg.eye.left
                              use(xlink:href='../assets/svg/icon-sprite.svg#eye')
                            svg.eye.right
                              use(xlink:href='../assets/svg/icon-sprite.svg#eye')

          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
